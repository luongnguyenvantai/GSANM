        {
          "datasource": {
            "type": "influxdb",
            "uid": "${DS_INFLUXDB}"
          },
          "query": "// Lấy dữ liệu số lần thất bại (Fails)\nfails = from(bucket: \"log_anomaly\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"behavior_metrics\")\n  |> filter(fn: (r) => r[\"_field\"] == \"ssh_failed_count\")\n  |> filter(fn: (r) => r._value > 0) // Chỉ lấy IP có tấn công SSH\n  |> aggregateWindow(every: 1h, fn: sum, createEmpty: false) // Tổng trong 1 giờ\n\n// Lấy dữ liệu số user riêng biệt (Users)\nusers = from(bucket: \"log_anomaly\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"behavior_metrics\")\n  |> filter(fn: (r) => r[\"_field\"] == \"ssh_distinct_users\")\n  |> filter(fn: (r) => r._value > 0)\n  |> aggregateWindow(every: 1h, fn: sum, createEmpty: false)\n\n// Kết hợp (join) hai bảng này lại dựa trên IP\njoin(\n  tables: {f: fails, u: users},\n  on: [\"source_ip\"]\n)\n// Tính toán cột mới 'ratio'\n|> map(fn: (r) => ({\n    r with\n    ratio: float(r._value_f) / float(r._value_u) // Lấy Fails / Users\n}))\n|> sort(columns: [\"ratio\"], desc: true) // Sắp xếp theo tỷ lệ\n|> rename(columns: {source_ip: \"IP Tan Cong\", _value_f: \"Tong so lan That bai\", _value_u: \"Tong so User\", ratio: \"Ty le (Fails/User)\"})",
          "refId": "A"
        }
